<h2>News</h2>
<br>
<% @articles.each do |article| %>
  <h3><%= article["title"] %></h3>
  <img src="<%= article["urlToImage"] %>" alt="">
  <p><%= article["description"] %></p><a href="<%= article["url"] %>" target="_blank">Read more</a>
  <p><%= article["author"] %></p>
  <p><%= article["publishedAt"] %></p>
  <br>
<% end %>
